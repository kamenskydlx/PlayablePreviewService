# Пароль для админки
ADMIN_PASSWORD=your_secure_password_here

# Базовый URL (замените на ваш домен)
BASE_URL=https://yourdomain.com

# Порт (по умолчанию 3000)
PORT=3000

# Секрет для сессий (ОБЯЗАТЕЛЬНО замените в продакшене!)
SESSION_SECRET=generate_your_own_64_char_random_string_here_for_production

# Режим работы (development или production)
NODE_ENV=production